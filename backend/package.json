{
    "name": "e-commerse",
    "version": "1.0.0",
    "description": "A comprehensive e-commerce REST API built with Node.js, Express.js, and MongoDB.",
    "main": "src/server.js",
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon src/server.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "format": "prettier --write src/",
        "seed": "node scripts/seed.js",
        "seed:admin": "node scripts/seed-admin.js",
        "create:admin": "node scripts/create-admin.js",
        "migrate": "node scripts/migrate.js",
        "setup:frontend": "node scripts/setup-frontend-port.js",
        "cors:add": "node scripts/cors-manager.js add",
        "cors:remove": "node scripts/cors-manager.js remove",
        "cors:show": "node scripts/cors-manager.js show",
        "cors:set-primary": "node scripts/cors-manager.js set-primary",
        "cors:reset": "node scripts/cors-manager.js reset"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.10.0",
        "bcryptjs": "^3.0.2",
        "cloudinary": "^2.7.0",
        "compression": "^1.8.0",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^4.21.2",
        "express-mongo-sanitize": "^2.2.0",
        "express-rate-limit": "^7.5.0",
        "express-validator": "^7.2.1",
        "helmet": "^8.1.0",
        "hpp": "^0.2.3",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.16.0",
        "morgan": "^1.10.0",
        "multer": "^2.0.1",
        "nodemailer": "^7.0.3",
        "sharp": "^0.34.2",
        "slugify": "^1.6.6",
        "stripe": "^18.2.1",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "winston": "^3.17.0",
        "xss-clean": "^0.1.4"
    },
    "devDependencies": {
        "eslint": "^9.29.0",
        "husky": "^9.1.7",
        "jest": "^30.0.2",
